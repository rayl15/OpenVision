// OpenVision Build Configuration
// Copy this file to Config.xcconfig and fill in your values

// Your Apple Developer Team ID
DEVELOPMENT_TEAM = YOUR_TEAM_ID

// Bundle identifier for the app
PRODUCT_BUNDLE_IDENTIFIER = com.yourcompany.openvision

// Meta App ID from https://developer.meta.com
// Required for glasses registration
META_APP_ID = YOUR_META_APP_ID

// Client Token from Meta Developer Console (FULL FORMAT REQUIRED)
// Format: AR|YOUR_META_APP_ID|YOUR_CLIENT_TOKEN
// Example: AR|1234567890|abcdef123456
CLIENT_TOKEN = AR|YOUR_META_APP_ID|YOUR_CLIENT_TOKEN

// URL scheme for Meta AI app callback
// Must match bundle identifier (replace dots with hyphens if needed)
APP_LINK_URL_SCHEME = openvision
